<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!--设置圆按钮的大小-->
    <size
        android:width="@dimen/sw_height"
        android:height="@dimen/sw_height" />
    <!--填充色-->
    <solid android:color="@color/white" />
    <!--边框的颜色的宽度-->
    <stroke
        android:width="@dimen/sw_thumb_frame"
        android:color="@color/themePositiveColor" />
    <!--圆角-->
    <corners android:radius="@dimen/sw_radius" />

</shape>
