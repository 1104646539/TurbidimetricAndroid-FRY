<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <variable
            name="item"
            type="com.wl.turbidimetric.model.TestResultModel" />
    </data>

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_item"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:padding="8dp">

        <ImageView
            android:id="@+id/iv_select"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/select_cb_query_data"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/tv_id"
            style="@style/TextHilt"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:text="ID" />

        <TextView
            android:id="@+id/tv_project_name"
            style="@style/TextHilt"
            android:layout_width="115dp"
            android:layout_height="wrap_content"
            android:minWidth="115dp"
            android:text="项目名"
            android:visibility="visible" />

        <TextView
            android:id="@+id/tv_barcode"
            style="@style/TextHilt"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:minWidth="100dp"
            android:text="条码"
            android:visibility="visible" />


        <TextView
            android:id="@+id/tv_name"
            style="@style/TextHilt"
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:text="姓名" />


        <TextView
            android:id="@+id/tv_gender"
            style="@style/TextHilt"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:text="性别" />


        <TextView
            android:id="@+id/tv_age"
            style="@style/TextHilt"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:text="年龄" />

        <TextView
            android:id="@+id/tv_detection_num"
            style="@style/TextHilt"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="编号" />

        <TextView
            android:id="@+id/tv_absorbances"
            style="@style/TextHilt"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="反应度" />

        <TextView
            android:id="@+id/tv_concentration"
            style="@style/TextHilt"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:text="浓度" />


        <TextView
            android:id="@+id/tv_result"
            style="@style/TextHilt"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="检测结果" />


        <TextView
            android:id="@+id/tv_test_time"
            style="@style/TextHilt"
            android:layout_width="220dp"
            android:layout_height="wrap_content"
            android:text="检测时间" />


        <TextView
            android:id="@+id/tv_test_value1"
            style="@style/TextHilt"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:text="第一次" />


        <TextView
            android:id="@+id/tv_test_value2"
            style="@style/TextHilt"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:text="第二次" />


        <TextView
            android:id="@+id/tv_test_value3"
            style="@style/TextHilt"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:text="第三次" />


        <TextView
            android:id="@+id/tv_test_value4"
            style="@style/TextHilt"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:text="第四次"
            android:visibility="gone" />

        <TextView
            android:id="@+id/tv_test_original_value1"
            style="@style/TextHilt"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:text="原始1" />


        <TextView
            android:id="@+id/tv_test_original_value2"
            style="@style/TextHilt"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:text="原始2" />


        <TextView
            android:id="@+id/tv_test_original_value3"
            style="@style/TextHilt"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:text="原始3"
            android:visibility="visible" />


        <TextView
            android:id="@+id/tv_test_original_value4"
            style="@style/TextHilt"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:text="原始4"
            android:visibility="gone" />

        <TextView
            android:id="@+id/tv_state_upload"
            style="@style/TextHilt"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="上传"
            android:visibility="visible" />

    </LinearLayout>
</layout>
