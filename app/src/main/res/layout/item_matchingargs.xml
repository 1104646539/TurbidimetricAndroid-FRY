<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

        <variable
            name="item"
            type="com.wl.turbidimetric.model.ProjectModel" />
    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:text="@{item.projectName}"
            android:textSize="18sp" />

        <TextView
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:text="@{String.valueOf(item.a1)}"
            android:textSize="18sp" />

        <TextView
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:text="@{String.valueOf(item.a2)}"
            android:textSize="18sp" />

        <TextView
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:text="@{String.valueOf(item.x0)}"
            android:textSize="18sp" />

        <TextView
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:text="@{String.valueOf(item.p)}"
            android:textSize="18sp" />

    </LinearLayout>
</layout>
