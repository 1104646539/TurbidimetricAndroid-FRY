<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    app:startDestination="@id/fragment_params">
    <fragment
        android:id="@+id/fragment_params"
        android:name="com.wl.turbidimetric.settings.params.ParamsFragment"
        tools:layout="@layout/fragment_params"></fragment>
    <fragment
        android:id="@+id/fragment_detection_num"
        android:name="com.wl.turbidimetric.settings.detectionnum.DetectionNumFragment"
        tools:layout="@layout/fragment_detection_num"></fragment>
    <fragment
        android:id="@+id/fragment_test_mode"
        android:name="com.wl.turbidimetric.settings.testmode.TestModeFragment"
        tools:layout="@layout/fragment_test_mode"></fragment>
    <fragment
        android:id="@+id/fragment_report"
        android:name="com.wl.turbidimetric.settings.report.ReportFragment"
        tools:layout="@layout/fragment_report"></fragment>
    <fragment
        android:id="@+id/fragment_network"
        android:name="com.wl.turbidimetric.settings.network.NetworkFragment"
        tools:layout="@layout/fragment_params"></fragment>

    <action
        android:id="@+id/to_fragment_params"
        app:destination="@id/fragment_params"
        app:launchSingleTop="true" />
    <action
        android:id="@+id/to_fragment_detection_num"
        app:destination="@id/fragment_detection_num"
        app:launchSingleTop="true" />
    <action
        android:id="@+id/to_fragment_test_mode"
        app:destination="@id/fragment_test_mode"
        app:launchSingleTop="true" />
    <action
        android:id="@+id/to_fragment_report"
        app:destination="@id/fragment_report"
        app:launchSingleTop="true" />
    <action
        android:id="@+id/to_fragment_network"
        app:destination="@id/fragment_network"
        app:launchSingleTop="true" />
</navigation>
