<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TextImageButton">
        <attr name="android:textColor" format="color"></attr>
        <attr name="android:textSize" format="dimension"></attr>
        <attr name="android:src" format="reference"></attr>
        <attr name="android:text" format="string"></attr>
        <attr name="android:textStyle" format="integer"></attr>
    </declare-styleable>
    <declare-styleable name="LeftNavigationView">
        <attr name="left_" format="string" />
        <attr name="custom_color" format="color" />
        <attr name="custom_size" format="dimension" />
    </declare-styleable>
    <declare-styleable name="SettingsItemView">
        <attr name="text" format="string" />
        <attr name="textSize" format="dimension" />
        <attr name="icon" format="reference" />
        <attr name="textColor" format="color" />
        <attr name="selectedTextColor" format="color" />
        <attr name="selectable" format="boolean" />
        <attr name="bg" format="reference" />
    </declare-styleable>
    <declare-styleable name="WlGroup">
        <attr name="ids" format="string" />
    </declare-styleable>

</resources>
